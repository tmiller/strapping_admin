<h1>Listing industries</h1>

<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th>Actions</th>
    </tr>
  </thead>
  <tbody id='industries'>
    <%= render @industries %>
  </tbody>
</table>
<div id="industries-pagination">
  <%= paginate(@industries, window: 2, theme: 'admin', remote:true)%>
</div>


<%= link_to 'New Industry', new_industry_path, class: 'btn btn-primary'%>
